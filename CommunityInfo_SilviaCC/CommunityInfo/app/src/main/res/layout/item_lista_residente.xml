<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:paddingTop="5dp"
    android:paddingBottom="18dp"
    android:paddingLeft="5dp"
    android:paddingRight="5dp"
    android:layout_marginTop="7dp"
    android:layout_marginBottom="5dp"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="15dp" >

    <TextView
        android:id="@+id/tv_txtDni"
        android:layout_width="55dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:paddingLeft="1dp"
        android:text="DNI: "
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tv_DNI"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/tv_txtDni"
        android:layout_marginStart="1dp"
        android:layout_marginTop="10dp"
        android:paddingLeft="1dp"
        android:text="DNI Residente"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_txtNombreResidente"
        android:layout_width="85dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_DNI"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:paddingLeft="1dp"
        android:text="Nombre: "
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="normal"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tv_NombreResidente"
        android:layout_width="270dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_DNI"
        android:layout_toRightOf="@+id/tv_txtNombreResidente"
        android:layout_marginStart="1dp"
        android:layout_marginTop="10dp"
        android:paddingLeft="1dp"
        android:text="Nombre: "
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="normal"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_txtNombreUser"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_NombreResidente"
        android:layout_marginStart="10dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="1dp"
        android:text="Nombre User:"
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="normal"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tv_NombreUserResidente"
        android:layout_width="235dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_NombreResidente"
        android:layout_toRightOf="@+id/tv_txtNombreUser"
        android:layout_marginStart="1dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="1dp"
        android:text="Nombre User:"
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="normal"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_txtTelefonoResidente"
        android:layout_width="85dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_NombreUserResidente"
        android:layout_marginStart="10dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="1dp"
        android:text="Teléfono: "
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="normal"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tv_TelefonoResidente"
        android:layout_width="270dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_NombreUserResidente"
        android:layout_toRightOf="@+id/tv_txtTelefonoResidente"
        android:layout_marginStart="1dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="1dp"
        android:text="Teléfono Residente"
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="normal"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_txtDireccionResidente"
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_TelefonoResidente"
        android:layout_marginStart="10dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="1dp"
        android:text="Dirección: "
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="normal"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tv_DireccionResidente"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_TelefonoResidente"
        android:layout_toRightOf="@+id/tv_txtDireccionResidente"
        android:layout_marginStart="1dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="1dp"
        android:text="Dirección Residente"
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="normal"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_txtEmailResidente"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_DireccionResidente"
        android:layout_marginStart="10dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="1dp"
        android:text="Email: "
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="normal"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tv_EmailResidente"
        android:layout_width="295dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_DireccionResidente"
        android:layout_toRightOf="@+id/tv_txtEmailResidente"
        android:layout_marginStart="1dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="1dp"
        android:text="Email Residente"
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="normal"
        tools:ignore="MissingConstraints" />

</RelativeLayout>